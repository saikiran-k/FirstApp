		<div class="clearfix"></div>
		<div id="container">
			<div id="notifications" class="hidden"></div>
			<div id="upcoming" class="hidden notifications">
			</div>
			<div id="new_contact" class="hidden panel">
				<header class="panel-heading no-margin"> Add Contact</header>
				<form id="contact_form" class="form-horizontal" role="form">
					<div class="form-group">
						<label for="firstName" class="col-xs-3 control-label">Name</label>
						<div class="col-xs-7">
						  <input type="text" class="form-control" id="firstName" name="first_name" placeholder="First Name">
						</div>
					</div>
				   <div class="form-group">
						<label for="lastName" class="col-xs-3 control-label"></label>
						<div class="col-xs-7">
						  <input type="text" class="form-control" id="lastName" name="last_name" placeholder="Last Name">
						</div>
					</div>
				  <div class="form-group">
					<label for="inputEmail3" class="col-xs-3 control-label">Email</label>
					<div class="col-xs-7">
					  <input type="email" class="form-control" id="contactEmail" name="email" placeholder="Email">
					</div>
				  </div>
				  <div class="form-group">
					<label for="phone" class="col-xs-3 control-label">Phone</label>
					<div class="col-xs-7">
					  <input type="text" class="form-control" id="contactPhone" name="phone" placeholder="Phone">
					</div>
				  </div>
				  <div class="form-group">
						<label for="jobTitle" class="col-xs-3 control-label">Job Title</label>
						<div class="col-xs-7">
						  <input type="text" class="form-control" id="jobTitle" name="title" placeholder="Job Title">
						</div>
					</div>
				   <div class="form-group">
						<label for="company" class="col-xs-3 control-label">Company</label>
						<div class="col-xs-7">
						  <input type="text" class="form-control" id="company" name="company" placeholder="Company">
						</div>
					</div>
				  <div class="form-group">
					<div class="col-xs-offset-3 col-xs-2" style="margin-top:10px;">
					  <button type="submit" id="save_contact" class="btn btn-primary">Save</button>
					</div>
				  </div>
				</form>	
			</div>
			<div id="new_task" class="hidden panel">
				<header class="panel-heading no-margin"> Add Task</header>
				<form id="task_form" class="form-horizontal" role="form">
					<div class="form-group">
						<label for="task" class="col-xs-3 control-label">Title</label>
						<div class="col-xs-7">
						  <input type="text" class="form-control" id="task" name="subject" placeholder="Title for Task">
						</div>
					</div>
				   <div class="form-group">
						<label for="category" class="col-xs-3 control-label">Category</label>
						<div class="col-xs-7 selectView">
                        <div class="styled-select">
							<select class="form-control selectpicker" data-style="btn-primary" id="category" name="type">
								<option value="CALL">Call</option>
								<option value="EMAIL">Email</option>
								<option value="FOLLOW_UP">Follow Up</option>
								<option value="MEETING">Meeting</option>
								<option value="MILESTONE">Milestone</option>
								<option value="SEND">Send</option>
								<option value="TWEET">Tweet</option>
								<option value="OTHER">Other</option>
							</select>
                            </div>
						</div>
					</div>
                   
				  <div class="form-group">
					<label for="priority_type" class="col-xs-3 control-label">Priority</label>
					<div class="col-xs-7 selectView">
                    <div class="styled-select">
						<select class="form-control" id="priority" name="priority_type">
							<option value="HIGH">High</option>
							<option value="NORMAL">Normal</option>
							<option value="LOW">Low</option>
						</select>
                        </div>
					</div>
				  </div>
				  <div class="form-group">
						<label for="owner_id" class="col-xs-3 control-label">Owner</label>
						<div class="col-xs-7 selectView">
                        <div class="styled-select">
							<select class="form-control" id="owner_id" name="owner_id">
								
							</select>
						</div>
                         </div>
					</div>
				  <div class="form-group">
						<label for="dueDate" class="col-xs-3 control-label">Due Date</label>
						<div class="col-xs-7">
						  <input type="text" class="form-control date" id="dueDate" name="due" placeholder="mm/dd/yyyy">
						</div>
					</div>
                   
				  <div class="form-group">
					<label for="relatedToEmail" class="col-xs-3 control-label">Related to</label>
					<div class="col-xs-7">
					  <input type="email" class="form-control" id="relatedToEmail" name="email" placeholder="Email of related contact">
					</div>
				  </div>
				  <div class="form-group">
					<div class="col-xs-offset-3 col-xs-2" style="margin-top:10px;">
					  <button type="submit" id="save_task" class="btn btn-primary">Save</button>
					</div>
				  </div>
				</form>	
			</div>
			<div id="new_deal" class="hidden panel">
				<header class="panel-heading no-margin"> Add Deal</header>
				<form id="deal_form" class="form-horizontal" role="form">
					<div class="form-group">
						<label for="deal_name" class="col-xs-3 control-label">Name</label>
						<div class="col-xs-7">
						  <input type="text" class="form-control" id="deal_name" name="name" placeholder="Name of the Deal">
						</div>
					</div>
					<div class="form-group">
						<label for="expected_value" class="col-xs-3 control-label">Value</label>
						<div class="col-xs-7">
						  <input type="text" class="form-control" id="expected_value" name="expected_value" placeholder="Expected Value">
						</div>
					</div>
					<div class="form-group">
						<label for="probability" class="col-xs-3 control-label">Probability</label>
						<div class="col-xs-7">
						  <input type="text" class="form-control" id="probability" name="probability" placeholder="Probability(%)">
						</div>
					</div>
				   <div class="form-group">
						<label for="milestone" class="col-xs-3 control-label">Milestone</label>
						<div class="col-xs-7 selectView">
                        <div class="styled-select">
							<select class="form-control" id="milestone" name="milestone">
								
							</select>
                            </div>
						</div>
					</div>
					 <div class="form-group">
						<label for="owner_id" class="col-xs-3 control-label">Owner</label>
						<div class="col-xs-7 selectView">
                        <div class="styled-select">
							<select class="form-control" id="owner_id" name="owner_id">
								
							</select>
                            </div>
						</div>
					</div>
				  <div class="form-group">
						<label for="close_date" class="col-xs-3 control-label">Close Date</label>
						<div class="col-xs-7">
						  <input type="text" class="form-control date" id="close_date" name="close_date" placeholder="mm/dd/yyyy">
						</div>
					</div>
				  <div class="form-group">
					<label for="relatedToEmail" class="col-xs-3 control-label">Related to</label>
					<div class="col-xs-7">
					  <input type="email" class="form-control" id="relatedToEmail" name="email" placeholder="Email of related contact">
					</div>
				  </div>
				  <div class="form-group">
					<label for="dealDescription" class="col-xs-3 control-label">Description</label>
					<div class="col-xs-7">
					  <textarea rows="2" class="form-control" id="dealDescription" name="description" placeholder="Description of the Deal"></textarea>
					</div>
				  </div>
				  <div class="form-group">
					<div class="col-xs-offset-3 col-xs-2" style="margin-top:10px;">
					  <button type="submit" id="save_deal" class="btn btn-primary">Save</button>
					</div>
				  </div>
				</form>	
			</div>
			<div id="crm_search" class="hidden agile-search"></div>
			<div id="crm_others" class="hidden"></div>
		</div>
		<script type="text/javascript" src="../js/lib/jquery.min.js"></script>
        
		<script type="text/javascript" src="../js/lib/bootstrap.min.js"></script>
         <script type="text/javascript" src="../js/bootstrap-select.js"></script>
         
		<script type="text/javascript" src="../js/lib/jquery.validate.min.js"></script>
		<script type="text/javascript" src="../js/lib/jquery-ui.min.js"></script>
        
		<script type="text/javascript" src="../js/lib/handlebars.min.js"></script>
		<script type="text/javascript" src="../js/lib/pubnub.min.js"></script>
		<script type="text/javascript" src="../js/lib/timeago.js"></script>
		<script type="text/javascript" src="../js/lib/md5.js"></script>
		<script type="text/javascript" src="../js/initial.min.js"></script>
		<script type="text/javascript" src="../js/content_country_code.js"></script>
		<script type="text/javascript" src="../js/popup.js"></script>
		<script type="text/javascript" src="../js/connector.js"></script>
		<script type="text/javascript" src="../js/populate_template.js"></script>
		
		<div id="templates">
			<script type="text/template" id="notification-template">
				{{#each content}}
					{{campaign_name}} - {{log_type}}<br/>
				{{/each}}
			</script>
			
			<script type="text/template" id="live-notification-template">
			
			{{#each agile_notifications}}
				{{#ifCondition notification "==" "OPENED_EMAIL"}}
				{{#if custom_value.time_opened}}
				<div class="panel-group" id="accordion">
				   <div class="panel panel-info">
					  <div class="panel-heading">
						   <p class="notification_header" data-toggle="collapse" data-parent="#accordion" data-target="#{{custom_value.time_opened}}">
								<strong> Subject : </strong>{{custom_value.email_subject}}
								Opened <time class="time-ago" datetime="{{epochToHumanDate "" custom_value.time_opened}}">{{epochToHumanDate "" custom_value.time_opened}}</time>
						   </p>
					  </div>
					  <div id="{{custom_value.time_opened}}" class="panel-collapse collapse">
						 <div class="panel-body">
							{{#if first_name}}
								{{first_name}}
							{{/if}}
							{{#if last_name}}
								{{last_name}}
							{{/if}}
							"{{email}}" opened email  
							<time class="time-ago" datetime="{{epochToHumanDate "" custom_value.time_opened}}">{{epochToHumanDate "" custom_value.time_opened}}</time>
							{{#if custom_value.time_sent}}
							which is sent on {{epochToTaskDate custom_value.time_sent}}
							{{/if}}
						 </div>
					  </div>
				   </div>
				</div>
				{{/if}}
				{{/ifCondition}}
				{{/each}}
			</script>
			
			<script type="text/template" id="upcoming-tasks-template">
				<ul class="striped-list" style="padding-left:0px;padding-right:20px;">
				<!-- li class="list-group-item active">Tasks</li -->
				{{#if content}}
					{{#each content}}
						 <li class="list-group-item" id="{{id}}" style="border:none;">
						
						<span >{{#if_equals priority_type "HIGH"}}
							<span class="label bordered-label"><i class="fa fa-exclamation red-exclamation"></i> &nbsp; {{ucfirst priority_type}}</span>
							{{/if_equals}}
							{{#if_equals priority_type "NORMAL"}}
							<span class="label bordered-label"><i class="fa fa-exclamation no-awesome-bg"></i> &nbsp; {{ucfirst priority_type}}</span-->
							{{/if_equals}}
							{{#if_equals priority_type "LOW"}}
							<span class="label bordered-label "><i class="fa fa-exclamation gray-exclamation"></i> &nbsp; {{ucfirst priority_type}}</span>
							{{/if_equals}}
							</span>
						 
<label class="checkbox checkbox-custom float-align" style="margin-top:2px"><input type="checkbox" class="complete_task" data-task="{{id}}"><i class="checkbox"></i></label>

							<span class="subject float-align" style="margin-top: 2px;width: 67%;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;">{{{subject}}}</span>
							<span class="label label-default float-align" style="float:right;">{{ucfirst type}}</span>
							
							<div class="clearfix"></div>
							<div class="related-to">
							{{#each contacts}}
							 <a href="https://{{../taskOwner.domain}}.agilecrm.com/#contact/{{id}}" target="_blank" id="notification-contact-id" title=" {{getPropertyValue properties "first_name"}} {{ getPropertyValue properties "last_name"}}">{{getPropertyValue properties "first_name"}} {{getPropertyValue properties "last_name"}}</a>
							{{/each}}
							</div>
							<div style="float:right;">
								<time class="time-ago small" datetime="{{timeago due}}"> {{timeago due}} </time>
							</div>
							<div class="clearfix"></div>
							</li>
							
					{{/each}}
				{{/if}}
				</table>
			</script>
			<script type="text/template" id="search-template">
			 <div class="col-lg-6">
				<div class="input-group">
				  <input type="search" class="form-control" id="inp_search_text" placeholder="Enter Search Keyword"  style="height:32px;">
				  <span class="input-group-btn">
					<button type="button" id="btn_search" data-loading-text="Searching..." class="btn agile-theme-search btn-primary" >
					  Search
					</button>
				  </span>
				</div><!-- /input-group -->
			  </div>
				<div id="search-results"></div>
			</script>
			<script type="text/template" id="search-results-template">
				{{#if content}}
					<div class="table table-condensed table-hover" style="margin: 6px 0;">
						
						{{#each content}}
							<div class="search-results" data-url="https://{{owner.domain}}.agilecrm.com/#contact/{{id}}">
								
								<div class="col-xs-2 no-left-padding">
									
										<img class="img-circle text_avatar" src="{{gravatarurl properties 50}}" data-name="{{getPropertyValue properties "first_name"}} {{ getPropertyValue properties "last_name"}}" width="50" height="50"/>
									</div>
									<div class="col-xs-10 no-left-padding" style="padding-right:0px;">
										<span id="notification-contact-id" class="font-weight-600 searched-title">{{getPropertyValue properties "first_name"}} {{getPropertyValue properties "last_name"}}</span>
										<div>
										{{getPropertyValue properties "email"}}
										</div>
										<div style="width: 100%;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;">
										{{{tags}}}
										</div>
									</div>
									<div class="clearfix"></div>
								
								
								
							</div>
						{{/each}}
					</div>
				{{/if}}
			</script>
			<script type='text/template' id='linkedin-active-template'>
				<span> Fetch contact from currently active linkedin page </span>
								&nbsp; <input type='button' value='Fetch' class="btn btn-primary" id='linkedinFetch' data-tabId='{{tabId}}'>			
			</script>
			<script type='text/template' id='facebook-active-template'>
				<span> Fetch contact from currently active Facebook page </span>
				&nbsp; <input type='button' value='Fetch' class="btn btn-primary" id='facebookFetch' data-tabId='{{tabId}}'>			
			</script>
			
			<script id="notify-html-template" type="text/html">
				{{#if class}}
					<div style="background-color: #f9f9f9" class="notifications-content"> 
				{{else}}
					<div class="notifications-content"> 
				 {{/if}} 
				<!-- Image urls hidden to show in desktop notifications. First span is for contact image and second span is for company.--> 
				<div class="col-xs-2 no-left-padding">
				<span id={{gravatarurl properties 50}}>
				{{#if properties}}
						{{#if_contact_type "PERSON"}}
						   <img class="img-circle text_avatar" src="{{gravatarurl properties 50}}" data-name="{{getPropertyValue properties "first_name"}} {{ getPropertyValue properties "last_name"}}" width="50" height="50"/>
						{{/if_contact_type}}
						{{#if_contact_type "COMPANY"}}
						  <img class="img-circle text_avatar" src="{{gravatarurl properties 50}}" data-name="{{getPropertyValue properties "name"}}" width="50" height="50"/>
						{{/if_contact_type}}
				{{else}}	
					<img class="img-circle {{notification}}" width="50" height="50"/>
				{{/if}}
					<i class="{{notification}}"></i>
				</span>
</div>
<div class="col-xs-10 no-left-padding" style="position:static;padding-right:0px;">
				<span id="https://www.google.com/s2/favicons?domain={{getPropertyValue properties "url"}}"></span>
					<span style="float:right">
						<time class="time-ago small" datetime="{{timeago notify_time}}"> {{timeago notify_time}} </time>
						
					</span>
				   {{#if entity_type}}
					   <span class="notifications-title-ellipsis"><a href="#deals" id="notification-deal-id" style="color:black;" class="font-weight-600">{{name}}</a></span>
				   {{else}}
					   {{#if_contact_type "PERSON"}}
						   <span style="vertical-align:text-top" class="notifications-title-ellipsis">
							   <a href="https://{{domain}}.agilecrm.com/#contact/{{id}}" class="font-weight-600" target="_blank" id="notification-contact-id" title=" {{getPropertyValue properties "first_name"}} {{ getPropertyValue properties "last_name"}}">{{getPropertyValue properties "first_name"}} {{getPropertyValue properties "last_name"}}</a>   
						   </span>
						{{/if_contact_type}}

						{{#if_contact_type "COMPANY"}}
						   <span style="vertical-align:text-top" class="notifications-title-ellipsis">
							   <a href="https://{{domain}}.agilecrm.com/#contact/{{id}}"  class="font-weight-600" target="_blank" id="notification-contact-id" style="color:black;font-weight:bold;">{{getPropertyValue properties "name"}}</a>
						   </span>  
						{{/if_contact_type}}
				   {{/if}} 

					<span class= "notification-type">{{#if_notification_type}} {{notification}} {{/if_notification_type}}</span>
					{{#if_equals notification "OPENED_EMAIL"}}
						<span class="small">
							{{{sentTime custom_value}}}
						</span>
					{{/if_equals}}
					
					</div>
					<div class="clearfix"></div>
				</div>

				</script>

				<script id="call-notification-template" type="text/html">
				<div class="notify notifications-content">
				<div class="col-xs-2 no-left-padding">
				<span id={{gravatarurl properties 50}}>
					<img src="{{gravatarurl properties 50}}" class="img-circle text_avatar" width="50" height="50" data-name="{{getPropertyValue properties "first_name"}} {{ getPropertyValue properties "last_name"}}"/>
				</span>
				</div>
				<div class="col-xs-10 no-left-padding" style="position: static;padding-right: 0px;">
				<span style="vertical-align:text-top">
					<a href="#contact/{{id}}" id = "calling-contact-id" style="color:black;font-weight:bold;" title=" {{getPropertyValue properties "first_name"}} {{ getPropertyValue properties "last_name"}}">{{getPropertyValue properties "first_name"}} {{getPropertyValue properties "last_name"}}</a>
				</span>
				<span style="margin-left:2px" id="call-notification-text">is calling</span>
				<span><time class="time-ago" datetime="{{timeago notify_time}}"> {{timeago notify_time}} </time></span>
				</div>
				<div class="clearfix"></div>
				</div>
				</script>

				<script id="campaign-notify-template" type="text/html">
				<div class="notify notifications-content">
					<div class="col-xs-2 no-left-padding">
				<span id={{gravatarurl properties 50}}>
					<img src="{{gravatarurl properties 50}}" class="img-circle text_avatar" width="50" height="50"  data-name="{{getPropertyValue properties "first_name"}} {{ getPropertyValue properties "last_name"}}"/>
				</span>
				</div>
				<div class="col-xs-10 no-left-padding" style="position: static;padding-right: 0px;">
								<span ><time class="time-ago" datetime="{{timeago notify_time}}"> {{timeago notify_time}} </time></span>

				<span style="vertical-align:text-top;" class="notifications-title-ellipsis">
					<a href="#contact/{{id}}" id = "campaign-contact-id" style="color:black;font-weight:600;" title=" {{getPropertyValue properties "first_name"}} {{ getPropertyValue properties "last_name"}}">{{getPropertyValue properties "first_name"}} {{getPropertyValue properties "last_name"}}</a>
				</span>
				
				<span class="notification-type" id="campaign-notify-text">{{#stringToJSON this "custom_value"}}{{msg}}{{/stringToJSON}}</span>
				</div>
				<div class="clearfix"></div>
				</div>
				</script>

				<!-- ----------Linked and FaceBook Forms----------- -->
				
				<script type="text/html" id="contact-add-template">
				<header class="panel-heading">Add Contact</header>
				<form class="form-horizontal" role="form" style="padding:10px;">
				<div class="form-group form-group-sm">
					<label class="col-xs-3 control-label">Name : </label>
					<div class="col-xs-9">
					<input name='first_name' type='text' class='_crmio_inp form-control' value='{{firstName}}' placeholder='First Name' style="margin-botton:5px;">
							<input name='last_name' type='text' class='_crmio_inp form-control' value='{{lastName}}' placeholder='Last Name'>
					</div>
				</div>
				<div class="form-group form-group-sm">
					<label class="col-xs-3 control-label">Company : </label>
					<div class="col-xs-9">
					<input name='company' type='text' class='_crmio_inp form-control' value='{{company}}' placeholder='Company'>
					</div>
				</div>
				<div class="form-group form-group-sm">
					<label class="col-xs-3 control-label">Job Title : </label>
					<div class="col-xs-9">
